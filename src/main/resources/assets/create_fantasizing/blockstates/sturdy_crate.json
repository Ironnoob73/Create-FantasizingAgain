{
  "multipart": [
    {"when": {"crate_type": "single"}, "apply": {"model": "create_fantasizing:block/crate/sturdy/single"}},
    {"when": {"crate_type": "main", "facing": "down"}, "apply": {"model": "create_fantasizing:block/crate/sturdy/top_main"}},
    {"when": {"crate_type": "main", "facing": "up"}, "apply": {"model": "create_fantasizing:block/crate/sturdy/bottom_main"}},
    {"when": {"crate_type": "main", "facing": "north"}, "apply": {"model": "create_fantasizing:block/crate/sturdy/right_main"}},
    {"when": {"crate_type": "main", "facing": "south"}, "apply": {"model": "create_fantasizing:block/crate/sturdy/left_main"}},
    {"when": {"crate_type": "main", "facing": "west"}, "apply": {"model": "create_fantasizing:block/crate/sturdy/left_main", "y": 90}},
    {"when": {"crate_type": "main", "facing": "east"}, "apply": {"model": "create_fantasizing:block/crate/sturdy/right_main", "y": 90}},
    {"when": {"crate_type": "second", "facing": "down"}, "apply": {"model": "create_fantasizing:block/crate/sturdy/top"}},
    {"when": {"crate_type": "second", "facing": "up"}, "apply": {"model": "create_fantasizing:block/crate/sturdy/bottom"}},
    {"when": {"crate_type": "second", "facing": "north"}, "apply": {"model": "create_fantasizing:block/crate/sturdy/right"}},
    {"when": {"crate_type": "second", "facing": "south"}, "apply": {"model": "create_fantasizing:block/crate/sturdy/left"}},
    {"when": {"crate_type": "second", "facing": "west"}, "apply": {"model": "create_fantasizing:block/crate/sturdy/left", "y": 90}},
    {"when": {"crate_type": "second", "facing": "east"}, "apply": {"model": "create_fantasizing:block/crate/sturdy/right", "y": 90}}
  ]
}