{
  "multipart": [
    {"when": {"crate_type": "single"}, "apply": {"model": "create_fantasizing:block/crate/iron/single"}},
    {"when": {"crate_type": "main", "facing": "down"}, "apply": {"model": "create_fantasizing:block/crate/iron/top_main"}},
    {"when": {"crate_type": "main", "facing": "up"}, "apply": {"model": "create_fantasizing:block/crate/iron/bottom_main"}},
    {"when": {"crate_type": "main", "facing": "north"}, "apply": {"model": "create_fantasizing:block/crate/iron/right_main"}},
    {"when": {"crate_type": "main", "facing": "south"}, "apply": {"model": "create_fantasizing:block/crate/iron/left_main"}},
    {"when": {"crate_type": "main", "facing": "west"}, "apply": {"model": "create_fantasizing:block/crate/iron/left_main", "y": 90}},
    {"when": {"crate_type": "main", "facing": "east"}, "apply": {"model": "create_fantasizing:block/crate/iron/right_main", "y": 90}},
    {"when": {"crate_type": "second", "facing": "down"}, "apply": {"model": "create_fantasizing:block/crate/iron/top"}},
    {"when": {"crate_type": "second", "facing": "up"}, "apply": {"model": "create_fantasizing:block/crate/iron/bottom"}},
    {"when": {"crate_type": "second", "facing": "north"}, "apply": {"model": "create_fantasizing:block/crate/iron/right"}},
    {"when": {"crate_type": "second", "facing": "south"}, "apply": {"model": "create_fantasizing:block/crate/iron/left"}},
    {"when": {"crate_type": "second", "facing": "west"}, "apply": {"model": "create_fantasizing:block/crate/iron/left", "y": 90}},
    {"when": {"crate_type": "second", "facing": "east"}, "apply": {"model": "create_fantasizing:block/crate/iron/right", "y": 90}}
  ]
}