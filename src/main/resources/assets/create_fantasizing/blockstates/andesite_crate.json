    {"when": {"crate_type": "second", "facing": "up"}, "apply": {"model": "create_fantasizing:block/crate/andesite/bottom"}},
    {"when": {"crate_type": "second", "facing": "north"}, "apply": {"model": "create_fantasizing:block/crate/andesite/right"}},
    {"when": {"crate_type": "second", "facing": "south"}, "apply": {"model": "create_fantasizing:block/crate/andesite/left"}},
    {"when": {"crate_type": "second", "facing": "west"}, "apply": {"model": "create_fantasizing:block/crate/andesite/left", "y": 90}},
    {"when": {"crate_type": "second", "facing": "east"}, "apply": {"model": "create_fantasizing:block/crate/andesite/right", "y": 90}}
  ]
}