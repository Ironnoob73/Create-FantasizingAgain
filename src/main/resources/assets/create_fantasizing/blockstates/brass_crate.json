{
  "multipart": [
    {"when": {"crate_type": "single"}, "apply": {"model": "create_fantasizing:block/crate/brass/single"}},
    {"when": {"crate_type": "main", "facing": "down"}, "apply": {"model": "create_fantasizing:block/crate/brass/top_main"}},
    {"when": {"crate_type": "main", "facing": "up"}, "apply": {"model": "create_fantasizing:block/crate/brass/bottom_main"}},
    {"when": {"crate_type": "main", "facing": "north"}, "apply": {"model": "create_fantasizing:block/crate/brass/right_main"}},
    {"when": {"crate_type": "main", "facing": "south"}, "apply": {"model": "create_fantasizing:block/crate/brass/left_main"}},
    {"when": {"crate_type": "main", "facing": "west"}, "apply": {"model": "create_fantasizing:block/crate/brass/left_main", "y": 90}},
    {"when": {"crate_type": "main", "facing": "east"}, "apply": {"model": "create_fantasizing:block/crate/brass/right_main", "y": 90}},
    {"when": {"crate_type": "second", "facing": "down"}, "apply": {"model": "create_fantasizing:block/crate/brass/top"}},
    {"when": {"crate_type": "second", "facing": "up"}, "apply": {"model": "create_fantasizing:block/crate/brass/bottom"}},
    {"when": {"crate_type": "second", "facing": "north"}, "apply": {"model": "create_fantasizing:block/crate/brass/right"}},
    {"when": {"crate_type": "second", "facing": "south"}, "apply": {"model": "create_fantasizing:block/crate/brass/left"}},
    {"when": {"crate_type": "second", "facing": "west"}, "apply": {"model": "create_fantasizing:block/crate/brass/left", "y": 90}},
    {"when": {"crate_type": "second", "facing": "east"}, "apply": {"model": "create_fantasizing:block/crate/brass/right", "y": 90}}
  ]
}